<Window x:Class="SeenEtiquetas.ContribuintesEditorWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Editar Contribuintes" Height="580" Width="350">
    <StackPanel Margin="15">
        <Label>Inscricao</Label>
        <TextBox x:Name="InscricaoTextBox"/>
        
        <Label>Contribuintes</Label>
        <TextBox x:Name="NomeTextBox"/>

        <Label>Dependente</Label>
        <TextBox x:Name="DependenteTextBox"/>

        <Label>Trabalho</Label>
        <TextBox x:Name="TrabalhoTextBox"/>

        <Label>Endereco</Label>
        <TextBox x:Name="EnderecoTextBox"/>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,20,0,0">
            <Button Content="Salvar" Click="Salvar_Click" IsDefault="True" Padding="10,5"/>
            <Button Content="Cancelar" Click="Cancelar_Click" IsCancel="True" Margin="10,0,0,0" Padding="10,5"/>
        </StackPanel>

    </StackPanel>
</Window>
